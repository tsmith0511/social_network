<h1>Edit</h1>

<%= form_for Profile.find(params[:id]), url: '/profile' do |f| %>
  <%= f.label :First_name %>
  <%= f.text_field :firstname %>
  <%= f.label :Last_name %>
  <%= f.text_field :lastname %>
  <br />
  <%= f.label :Age %>
  <%= f.text_field :age %>
  <%= f.label :Gender %>
  <%= f.text_field :gender %>
  <%= f.submit "Save" %>
<% end %>